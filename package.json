{
  "name": "jsonata",
  "type": "module",
  "version": "1.0.0",
  "description": "A project using JSONata and TypeScript",
  "main": "dist/main.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "watch:ts": "tsc --watch --project tsconfig.json",
    "start": "tsc && node dist/main.js",
    "watch": "tsc && nodemon --watch . --ext js,ts,json,jsonata --exec node dist/main.js",
    "watch:sec": "tsc && nodemon --watch . --ext js,ts,json,jsonata --exec node dist/main-sec.js",
    "watch:validation": "tsc && nodemon --watch . --ext js,ts,json,jsonata --exec node dist/main-validation.js",
    "jsonata": "tsc && node dist/run-jsonata.js",
    "jsonata:watch": "tsc && nodemon --watch . --ext js,ts,json,jsonata --exec node dist/run-jsonata.js",
    "jsonata:watch:sh": "./scripts/watch-jsonata.sh",
    "demo": "tsc && node dist/demo.js",
    "demo:watch": "tsc && nodemon --watch . --ext js,ts --exec node dist/demo.js",
    "test:validation": "tsc && node dist/validation-test.js",
    "schema:validation": "ts-node src/schemas/dakosy.schema.ts",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "check": "biome check --write --unsafe src/"
  },
  "dependencies": {
    "dayjs": "^1.11.19",
    "jsonata": "^2.1.0",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.0.4",
    "@vitest/ui": "^4.0.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^4.0.16"
  }
}
